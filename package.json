{
  "name": "api-doc-manager",
  "version": "1.0.0",
  "description": "API 문서 버전 관리 시스템 (API Doc Version Manager)",
  "main": "server.js",
  "scripts": {
    "start": "node --max-old-space-size=2048 server.js",
    "dev": "nodemon server.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "swagger",
    "api",
    "documentation",
    "version-control"
  ],
  "author": "Taejin Kim",
  "license": "ISC",
  "dependencies": {
    "@slack/web-api": "^7.0.0",
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "json-diff": "^1.0.6",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
